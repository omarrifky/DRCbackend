package com.example.demo;

import java.io.Serializable;

public class AnnualorCasualRequest extends Request implements Serializable {
String Type;
String name; //annual or casual
public AnnualorCasualRequest (String t , String d , String r ,String e , String s,String i,String m,String c) {
	super(  d ,  r , e ,  s, i,c);
	name = m;
	Type = t;
}

}
